{"components/faceted-search/facets/rating":"<ul class=\"faceted-search-filter-list show-filter-list {{#if selected }}is-open{{/if}} {{#unless start_collapsed }}is-open{{/unless}}\">\n  {{#each items}}\n    <li class=\"faceted-search-item\">\n      <a href=\"{{url}}\" title=\"{{lang 'product.reviews.stars_count' NUM=title}}\" rel=\"nofollow\" data-faceted-search-facet>\n\n        <input type=\"checkbox\" class=\"facet-checkbox\" {{#if selected}}checked{{/if}}>\n\n        <span class=\"faceted-search-item-label facet-rating\">\n\n          {{#if title '==' '1'}}\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star\"></span>\n            <span class=\"fa fa-star\"></span>\n            <span class=\"fa fa-star\"></span>\n            <span class=\"fa fa-star\"></span>\n          {{/if}}\n\n          {{#if title '==' '2'}}\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star\"></span>\n            <span class=\"fa fa-star\"></span>\n            <span class=\"fa fa-star\"></span>\n          {{/if}}\n\n          {{#if title '==' '3'}}\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star\"></span>\n            <span class=\"fa fa-star\"></span>\n          {{/if}}\n\n          {{#if title '==' '4'}}\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star\"></span>\n          {{/if}}\n\n          {{#if title '==' '5'}}\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star checked\"></span>\n          {{/if}}\n\n          {{#if ../show_product_counts}}<span class=\"faceted-search-item-count\">({{ count }})</span>{{/if}}\n\n        </span>\n\n      </a>\n    </li>\n  {{/each}}\n</ul>\n"}
