{"components/snippets/newsletter-signup":"<section class=\"layout-block newsletter-signup-block\">\n  <div class=\"newsletter-signup\">\n    <div class=\"newsletter-signup-wrap\">\n\n      <h2 class=\"section-title hidden\">{{lang 'newsletter.heading'}}</h2>\n\n      <div class=\"newsletter-signup-form\">\n        <p class=\"newsletter-signup-label\">{{lang 'newsletter.label'}}</p>\n        <form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n          <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n          <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n          <input type=\"hidden\" name=\"check\" value=\"1\">\n          <label class=\"form-label show-for-sr\" for=\"nl_email\">{{lang 'newsletter.email'}}</label>\n          <input class=\"newsletter-signup-email form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email'}}\">\n          <div class=\"newsletter-signup-submit\">\n            <input class=\"submit button\" type=\"submit\" value=\"{{lang 'newsletter.subscribe'}}\">\n          </div>\n        </form>\n      </div>\n\n    </div>\n  </div>\n</section>\n"}
