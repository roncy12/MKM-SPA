{"components/alerts/alert-error":"<div class=\"alert alert-error\">\n  {{{this}}}\n</div>\n","components/alerts/alert-success":"<div class=\"alert alert-success\">\n  {{{this}}}\n</div>\n","components/common/spinner":"<div class=\"spinner\">\n  <div class=\"spinner-circle1 spinner-circle\"></div>\n  <div class=\"spinner-circle2 spinner-circle\"></div>\n  <div class=\"spinner-circle3 spinner-circle\"></div>\n  <div class=\"spinner-circle4 spinner-circle\"></div>\n  <div class=\"spinner-circle5 spinner-circle\"></div>\n  <div class=\"spinner-circle6 spinner-circle\"></div>\n  <div class=\"spinner-circle7 spinner-circle\"></div>\n  <div class=\"spinner-circle8 spinner-circle\"></div>\n  <div class=\"spinner-circle9 spinner-circle\"></div>\n  <div class=\"spinner-circle10 spinner-circle\"></div>\n  <div class=\"spinner-circle11 spinner-circle\"></div>\n  <div class=\"spinner-circle12 spinner-circle\"></div>\n</div>\n","components/products/bulk-pricing":"<div id=\"product-bulk-pricing-modal\">\n  <h3 class=\"modal-title\">{{lang 'product.bulk_pricing.title'}}</h3>\n  <p>{{lang 'product.bulk_pricing.instructions'}}</p>\n  <ul>\n    {{#each product.bulk_discount_rates}}\n      <li>\n        {{lang 'product.bulk_pricing.range' min=min max=max}}\n        {{#if type \"===\" \"percent\"}}\n          {{lang 'product.bulk_pricing.percent' discount=discount.formatted}}\n        {{/if}}\n        {{#if type \"===\" \"fixed\"}}\n          {{lang 'product.bulk_pricing.fixed' discount=discount.formatted}}\n        {{/if}}\n        {{#if type \"===\" \"price\"}}\n          {{lang 'product.bulk_pricing.price' discount=discount.formatted}}\n        {{/if}}\n      </li>\n    {{/each}}\n  </ul>\n</div>\n","components/products/customizations/checkbox":"<div class=\"form-field form-field-customizations form-field-inline form-field-checkbox{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-control\">\n      <input class=\"form-input form-checkbox\"\n        id=\"product-field-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        type=\"checkbox\"\n        {{#if value}}checked{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n      <span class=\"form-label-text product-customization-text\">{{display_name}}</span>\n    </span>\n  </label>\n</div>\n","components/products/customizations/file":"<div class=\"form-field form-field-customizations form-field-file{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-file-wrapper\" data-button-label=\"{{lang 'product.upload'}}\">\n        <input\n          class=\"form-input form-file\"\n          id=\"product-fields-{{id}}\"\n          name=\"ProductFields[{{this.id}}]\"\n          type=\"file\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n      </span>\n    </span>\n    <span class=\"form-file-types\">{{lang 'forms.validation.accepted_file_types' file_types=file_types}}</span>\n  </label>\n</div>\n","components/products/customizations/select":"<div class=\"form-field form-field-customizations form-field-inline form-field-select{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper\">\n        <select\n          class=\"form-input input-select\"\n          id=\"product-fields-{{id}}\"\n          name=\"ProductFields[{{this.id}}]\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n          {{#each values}}\n            <option value=\"{{this}}\" {{#if selected}}selected{{/if}}>{{this}}</option>\n          {{/each}}\n        </select>\n      </span>\n    </span>\n  </label>\n</div>\n","components/products/customizations/text":"<div class=\"form-field form-field-customizations form-field-inline form-field-text{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\" for=\"product-fields-{{id}}\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"product-fields-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        type=\"text\"\n        placeholder=\"{{prefill}}\"\n        value=\"{{value}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","components/products/customizations/textarea":"<div class=\"form-field form-field-customizations form-field-textarea{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}:\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <textarea\n        class=\"form-input\"\n        id=\"product-fields-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        rows=\"4\"\n        placeholder=\"{{prefill}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\"></textarea>\n    </span>\n  </label>\n</div>\n","components/products/options/date":"<div class=\"form-field form-field-options form-field-date{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"date\">\n  <label class=\"form-label\">\n\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper form-date-year\">\n        <select class=\"form-input form-select\" name=\"attribute[{{id}}][year]\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.year'}}</option>\n          {{#enumerate earliest_year latest_year}}\n            <option value=\"{{this}}\" {{#equals ../selected_date.year this}}selected=\"selected\"{{/equals}}>{{this}}</option>\n          {{/enumerate}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-month\">\n        <select class=\"form-input form-select\" name=\"attribute[{{id}}][month]\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.month'}}</option>\n          {{#enumerate 1 12}}\n            <option value=\"{{this}}\" {{#equals ../selected_date.month this}}selected=\"selected\"{{/equals}}>{{getShortMonth this}}</option>\n          {{/enumerate}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-day\">\n        <select class=\"form-input form-select\" name=\"attribute[{{id}}][day]\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.day'}}</option>\n          {{#enumerate 1 31}}\n            <option value=\"{{this}}\" {{#equals ../selected_date.day this}}selected=\"selected\"{{/equals}}>{{this}}</option>\n          {{/enumerate}}\n        </select>\n        </span>\n    </span>\n\n  </label>\n</div>\n","components/products/options/input-checkbox":"<div class=\"form-field form-field-options form-field-inline form-field-checkbox{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"input-checkbox\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    <label class=\"form-label {{class}}\">\n      <input\n        class=\"form-input form-checkbox\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        type=\"checkbox\"\n        value=\"{{value}}\"\n        {{#if checked}}checked{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n      <span class=\"form-label-text\">{{label}}</span>\n    </label>\n  </div>\n</div>\n","components/products/options/input-file":"<div class=\"form-field form-field-options form-field-file{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"input-file\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-file-wrapper\" data-button-label=\"{{lang 'product.upload'}}\">\n        <input\n          class=\"form-input form-file\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{id}}]\"\n          type=\"file\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n      </span>\n    </span>\n    {{#if file_types}}\n      <span class=\"form-file-types\">{{lang 'forms.validation.accepted_file_types' file_types=file_types}}</span>\n    {{else}}\n      <span class=\"form-file-types\">{{lang 'forms.validation.all_accepted_file_types'}}</span>\n    {{/if}}\n  </label>\n</div>\n","components/products/options/input-numbers":"<div class=\"form-field form-field-options form-field-inline form-field-number{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"input-number\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\" id=\"attribute-{{id}}\" name=\"attribute[{{id}}]\" type=\"number\" value=\"{{prefill}}\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","components/products/options/input-text":"<div class=\"form-field form-field-options form-field-inline form-field-text{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"input-text\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        type=\"text\"\n        placeholder=\"{{prefill}}\"\n        value=\"\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","components/products/options/product-list":"\n<div class=\"form-field form-field-options form-field-product-list{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"product-list\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    {{#unless values.0.image}}\n      {{!-- same as radio buttons if no image set --}}\n      {{#each values}}\n        <label class=\"form-label form-field-radio product-picklist-no-image\" data-product-attribute-value=\"{{id}}\">\n          <input\n            class=\"form-input form-radio product-picklist-radio\"\n            name=\"attribute[{{../id}}]\"\n            type=\"radio\"\n            value=\"{{id}}\"\n            {{#if selected}}checked{{/if}}\n            {{#if ../required}}required{{/if}}\n            aria-required=\"{{required}}\">\n          <span class=\"form-label-text\">{{label}}</span>\n        </label>\n      {{/each}}\n      <label class=\"form-label form-field-radio product-picklist-no-image\">\n        <input\n          class=\"form-input form-radio product-picklist-radio\"\n          name=\"attribute[{{id}}]\"\n          type=\"radio\" value=\"0\"\n          {{#if defaultValue '==' 0}}checked{{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n        <span class=\"form-label-text\">{{lang 'product.none'}}</span>\n      </label>\n    {{else}}\n      {{#each values}}\n        <label class=\"form-label form-field-radio product-picklist-item\" data-product-attribute-value=\"{{id}}\">\n          <input\n            class=\"form-input form-radio product-picklist-radio\"\n            name=\"attribute[{{../id}}]\"\n            type=\"radio\" value=\"{{id}}\"\n            {{#if selected}}checked{{/if}}\n            {{#if ../required}}required{{/if}}\n            aria-required=\"{{required}}\">\n          <span class=\"product-picklist-item-body\">\n            <span class=\"product-picklist-image-wrap\">\n              <img class=\"product-picklist-image\" src=\"{{getImage image 'product_thumbnail'}}\" alt=\"{{image.alt}}\">\n            </span>\n            <span class=\"product-picklist-label-text form-label-text\">{{label}}</span>\n          </span>\n        </label>\n      {{/each}}\n      <label class=\"form-label form-field-radio product-picklist-item\">\n        <input\n          class=\"form-input form-radio product-picklist-radio\"\n          name=\"attribute[{{id}}]\"\n          type=\"radio\"\n          value=\"0\"\n          {{#if defaultValue '==' 0}}checked{{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n        <span class=\"product-picklist-item-body picklist-none\">\n          <span class=\"product-picklist-label-text form-label-text\">{{lang 'product.none'}}</span>\n        </span>\n      </label>\n    {{/unless}}\n  </div>\n</div>\n","components/products/options/set-radio":"<div class=\"form-field form-field-options form-field-radio form-field-inline {{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"set-radio\">\n  <div class=\"form-field-title product-option-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n  <div class=\"form-field-control\">\n    {{#each values}}\n      <label class=\"form-label\" data-product-attribute-value=\"{{id}}\">\n        <input\n          class=\"form-input form-radio\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{../id}}]\"\n          type=\"radio\"\n          value=\"{{id}}\"\n          {{#if selected}}checked{{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n        <span class=\"form-label-text\">{{label}}</span>\n      </label>\n    {{/each}}\n  </div>\n</div>\n","components/products/options/set-rectangle":"<div class=\"form-field form-field-options form-field-inline form-field-rectangle{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"set-rectangle\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n  <div class=\"form-field-control\">\n    {{#each values}}\n      <label class=\"form-label rectangle\" data-product-attribute-value=\"{{id}}\">\n        <input\n          class=\"form-input form-rectangle\"\n          name=\"attribute[{{../id}}]\"\n          id=\"attribute-{{id}}\"\n          type=\"radio\"\n          value=\"{{id}}\"\n          {{#if selected}}checked{{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n        <span class=\"rectangle-text form-label-text\">{{label}}</span>\n      </label>\n    {{/each}}\n  </div>\n</div>\n","components/products/options/set-select":"<div class=\"form-field form-field-options form-field-inline form-field-select{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"set-select\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <div class=\"form-select-wrapper\">\n        <select\n          class=\"form-input form-select\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{id}}]\"\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n          <option value=\"\">{{lang 'product.pick_one'}}</option>\n          {{#each values}}\n            <option value=\"{{id}}\" {{#if selected}}selected{{/if}} data-product-attribute-value=\"{{id}}\">{{label}}</option>\n          {{/each}}\n        </select>\n      </div>\n    </span>\n  </label>\n</div>\n","components/products/options/swatch":"<div class=\"form-field form-field-options form-field-inline form-field-swatch{{#if required}} form-required{{/if}}\"\n  data-swatch-selector\n  data-product-attribute=\"swatch\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    {{#each values}}\n      <label class=\"swatch-wrap\" data-swatch-value=\"{{label}}\" data-product-attribute-value=\"{{id}}\">\n        <span class=\"form-label-text\">{{label}}</span>\n\n        <input\n          class=\"form-input swatch-radio\"\n          id=\"attribute-{{id}}\"\n          type=\"radio\"\n          name=\"attribute[{{../id}}]\"\n          value=\"{{id}}\"\n          {{#if selected}}checked{{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n\n        <span class=\"swatch {{#if data.[1]}}two-colors{{/if}}{{#if data.[2]}} three-colors{{/if}}\">\n          {{#if pattern}}\n            <span class=\"swatch-color swatch-pattern\" style=\"background-image: url('{{getImage image 'product_thumbnail'}}');\"></span>\n          {{/if}}\n\n          {{#if data.[0]}}\n            <span class=\"swatch-color\" style=\"background-color: #{{data.[0]}}\"></span>\n          {{/if}}\n\n          {{#if data.[1]}}\n            <span class=\"swatch-color secondary\" style=\"background-color: #{{data.[1]}}\"></span>\n          {{/if}}\n\n          {{#if data.[2]}}\n            <span class=\"swatch-color tertiary\" style=\"background-color: #{{data.[2]}}\"></span>\n          {{/if}}\n        </span>\n\n        {{#if pattern}}\n          <span class=\"swatch-pattern-expanded\">\n            <img class=\"swatch-pattern-image\" src=\"{{getImage image 'product_item'}}\">\n          </span>\n        {{/if}}\n\n      </label>\n    {{/each}}\n  </div>\n\n  <span class=\"swatch-value\" data-swatch-value>{{lang 'forms.swatches_none_selected'}}</span>\n\n</div>\n","components/products/options/textarea":"<div class=\"form-field form-field-options form-field-textarea{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"textarea\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <textarea\n        class=\"form-input\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        rows=\"4\"\n        placeholder=\"{{prefill}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\"></textarea>\n    </span>\n  </label>\n</div>\n","components/products/price":"<div class=\"product-price\">\n  {{#if price.with_tax}}\n    <span class=\"price-with-tax\">\n      <span data-product-price-wrapper=\"with-tax\">\n        <span data-product-price>\n          {{price.with_tax.formatted}}\n        </span>\n\n        {{#if price.saved}}\n          <span class=\"price-rrp\" data-product-rrp>\n            {{price.rrp_with_tax.formatted}}\n          </span>\n        {{/if}}\n      </span>\n\n      {{#if price.without_tax}}\n        <span class=\"price-tax-label\">{{lang 'product.including_tax'}}</span>\n      {{/if}}\n    </span>\n  {{/if}}\n\n  {{#if price.without_tax}}\n    <span class=\"price-without-tax\">\n      <span data-product-price-wrapper=\"without-tax\">\n        <span data-product-price>\n          {{price.without_tax.formatted}}\n        </span>\n\n        {{#if price.saved}}\n          <span class=\"price-rrp\" data-product-rrp>\n            {{price.rrp_without_tax.formatted}}\n          </span>\n        {{/if}}\n      </span>\n\n      {{#if price.with_tax}}\n        <span class=\"price-tax-label\">{{lang 'product.excluding_tax'}}</span>\n      {{/if}}\n    </span>\n  {{/if}}\n\n  {{#if show_savings}}\n    {{#if price.saved}}\n      <span class=\"price-saved\" data-product-price-saved>\n        ({{lang 'product.you_save'}} {{price.saved.formatted}})\n      </span>\n    {{/if}}\n  {{/if}}\n</div>\n","components/products/product/additional-details":"<div class=\"single-product-meta-wrap\">\r\n  {{#if product.stock_level}}\r\n    <div class=\"single-product-meta-item single-product-meta-item-stock\">\r\n      <span class=\"single-product-meta-item-label\">{{lang 'products.current_stock'}}:</span>\r\n      <span class=\"single-product-meta-item-value\">{{product.stock_level}}</span>\r\n    </div>\r\n  {{/if}}\r\n  {{#if product.upc}}\r\n    <div class=\"single-product-meta-item single-product-meta-item-upc\">\r\n      <span class=\"single-product-meta-item-label\">{{lang 'product.upc'}}:</span>\r\n      <span class=\"single-product-meta-item-value\" data-product-upc>{{product.upc}}</span>\r\n    </div>\r\n  {{/if}}\r\n  {{#if product.release_date}}\r\n    <div class=\"single-product-meta-item single-product-meta-item-release-date\">\r\n      <span class=\"single-product-meta-item-label\">{{lang 'product.release_date'}}:</span>\r\n      <span class=\"single-product-meta-item-value\">{{product.release_date}}</span>\r\n    </div>\r\n  {{/if}}\r\n\r\n  {{#if product.condition}}\r\n    <div class=\"single-product-meta-item single-product-meta-item-condition\">\r\n      <span class=\"single-product-meta-item-label\">{{lang 'product.condition'}}:</span>\r\n      <span class=\"single-product-meta-item-value\">{{product.condition}}</span>\r\n    </div>\r\n  {{/if}}\r\n  {{#if product.availability}}\r\n    <div class=\"single-product-meta-item single-product-meta-item-availability\">\r\n      <span class=\"single-product-meta-item-label\">{{lang 'product.availability'}}:</span>\r\n      <span class=\"single-product-meta-item-value\">{{product.availability}}</span>\r\n    </div>\r\n  {{/if}}\r\n  {{#if product.warranty}}\r\n    <div class=\"single-product-meta-item single-product-meta-item-warranty\">\r\n      <span class=\"single-product-meta-item-label\">{{lang 'product.warranty'}}:</span>\r\n      <span class=\"single-product-meta-item-value\">{{nl2br product.warranty}}</span>\r\n    </div>\r\n  {{/if}}\r\n  {{#if product.shipping}}\r\n    {{#if product.shipping.calculated}}\r\n      <div class=\"single-product-meta-item single-product-meta-item-shipping\">\r\n        <span class=\"single-product-meta-item-label\">{{lang 'product.shipping'}}:</span>\r\n        <span class=\"single-product-meta-item-value\">{{lang 'product.shipping_calculated'}}</span>\r\n      </div>\r\n    {{else}}\r\n      {{#compare product.shipping.price 0 operator='==='}}\r\n        <div class=\"single-product-meta-item single-product-meta-item-shipping\">\r\n          <span class=\"single-product-meta-item-label\">{{lang 'product.shipping'}}:</span>\r\n          <span class=\"single-product-meta-item-value\">{{lang 'product.shipping_free'}}</span>\r\n        </div>\r\n      {{else}}\r\n        <div class=\"single-product-meta-item single-product-meta-item-shipping\">\r\n          <span class=\"single-product-meta-item-label\">{{lang 'product.shipping'}}:</span>\r\n          <span class=\"single-product-meta-item-value\">{{lang 'product.shipping_fixed' amount=product.shipping.price.formatted}}</span>\r\n        </div>\r\n      {{/compare}}\r\n    {{/if}}\r\n  {{/if}}\r\n  {{#if product.bulk_discount_rates.length}}\r\n    <div class=\"single-product-meta-item single-product-meta-item-bulk-discounts single-product-bulk-discounts\">\r\n      <span class=\"single-product-meta-item-label\">{{lang 'product.bulk_pricing.title'}}</span>\r\n      <span class=\"single-product-meta-item-value\">\r\n        <a href=\"#\" class=\"product-bulk-pricing-toggle\">{{lang 'product.bulk_pricing.see_pricing'}}</a>\r\n      </span>\r\n    </div>\r\n  {{/if}}\r\n\r\n  {{#if product.detail_messages}}\r\n    <div class=\"single-product-meta-item single-product-meta-item-detail-messages\">\r\n      <span class=\"single-product-meta-item-label\">{{lang 'product.message'}}</span>\r\n      <span class=\"single-product-meta-item-value\">\r\n        {{product.detail_messages}}\r\n      </span>\r\n    </div>\r\n  {{/if}}\r\n</div>\r\n","components/products/product/product-details":"<div class=\"product-details\" data-product-details data-product-title=\"{{product.title}}\" itemscope itemtype=\"http://schema.org/Product\">\r\n  <div class=\"single-product-alerts\">\r\n    <div class=\"product-message alert\" data-product-message></div>\r\n\r\n    {{#each product.reviews.messages}}\r\n      {{#if error}}\r\n        {{> components/alerts/alert-error this.error}}\r\n      {{/if}}\r\n      {{#if success}}\r\n        {{> components/alerts/alert-success this.success}}\r\n      {{/if}}\r\n    {{/each}}\r\n\r\n    {{#if product.out_of_stock}}\r\n      {{#if product.out_of_stock_message}}\r\n        {{> components/alerts/alert-error product.out_of_stock_message}}\r\n      {{else}}\r\n        {{> components/alerts/alert-error (lang 'products.sold_out')}}\r\n      {{/if}}\r\n    {{/if}}\r\n  </div>\r\n\r\n  <div class=\"single-product-information\">\r\n\r\n\r\n    {{#if titletag}}\r\n    <h2 class=\"single-product-title\">\r\n      <span itemprop=\"name\">{{product.title}}</span>\r\n    </h2>\r\n    {{else}}\r\n    <h1 class=\"single-product-title\">\r\n      <span itemprop=\"name\">{{product.title}}</span>\r\n    </h1>\r\n    {{/if}}\r\n    <meta itemprop=\"image\" content=\"{{getImage product.main_image}}\">\r\n    <meta itemprop=\"sku\" content=\"{{product.sku}}\">\r\n    <meta itemprop=\"description\" content=\"{{product.description}}\">\r\n\r\n    {{#if settings.show_product_rating}}\r\n      <div class=\"single-product-rating\"{{#if product.num_reviews '>' 0}} itemprop=\"aggregateRating\" itemscope itemtype=\"http://schema.org/AggregateRating\"{{/if}}>\r\n        {{#if product.num_reviews '>' 0}}\r\n    \t<meta itemprop=\"ratingValue\" content=\"{{product.rating}}\">\r\n    \t<meta itemprop=\"reviewCount\" content=\"{{product.num_reviews}}\">\r\n        {{/if}}\r\n        {{> components/products/ratings rating=product.rating size=\"medium\"}}\r\n        {{#if product.num_reviews}}<a href=\"#product-reviews\" class=\"review-cnt\">({{product.num_reviews}} product reviews)</a>{{else}}<a href=\"#modal-review-form\" class=\"review-link\">{{ lang 'product.reviews.leave_review' }}</a>{{/if}}\r\n      </div>\r\n\r\n      {{{snippet 'product_rating'}}}\r\n    {{/if}}\r\n  {{#if product.sku}}\r\n    <div class=\"single-product-meta-item single-product-meta-item-sku\">\r\n      <span class=\"single-product-meta-item-label\">{{lang 'product.sku'}}:</span>\r\n      <span class=\"single-product-meta-item-value\" data-product-sku>{{product.sku}}</span>\r\n    </div>\r\n  {{/if}}\r\n    <div class=\"single-product-price\">\r\n      {{#if product.call_for_price}}\r\n        <div class=\"product-call-for-price\">\r\n          {{product.call_for_price}}\r\n        </div>\r\n      {{else}}\r\n        {{> components/products/price context=\"product\" price=product.price show_savings=true}}\r\n      {{/if}}\r\n    </div>\r\n    <div itemprop=\"brand\" itemscope itemtype=\"http://schema.org/Thing\"><meta itemprop=\"name\" content=\"{{#if product.brand.name}}{{product.brand.name}}{{else}}{{settings.store_name}}{{/if}}\"></div>\r\n    {{#if product.brand.name}}\r\n    <div class=\"single-product-meta-item\">\r\n      <span class=\"single-product-meta-item-label\">Brand:</span>\r\n      <span class=\"single-product-meta-item-value\"><a href=\"{{product.brand.url}}\">{{product.brand.name}}</a></span>\r\n    </div>\r\n    {{/if}}\r\n  {{#if product.weight}}\r\n    <div class=\"single-product-meta-item single-product-meta-item-weight\">\r\n      <span class=\"single-product-meta-item-label\">{{lang 'product.weight'}}:</span>\r\n      <span class=\"single-product-meta-item-value\" data-product-weight>{{product.weight}}</span>\r\n    </div>\r\n  {{/if}}\r\n\r\n    <div itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\">\r\n      <meta itemprop=\"priceCurrency\" content=\"{{currency_selector.active_currency_code}}\">\r\n      <meta itemprop=\"price\" content=\"{{#if product.price.without_tax}}{{product.price.without_tax.value}}{{/if}}{{#if product.price.with_tax}}{{#unless product.price.without_tax}}{{product.price.with_tax.value}}{{/unless}}{{/if}}\">\r\n      <meta itemprop=\"itemCondition\" content=\"{{#if product.condition '===' 'New'}}http://schema.org/NewCondition{{/if}}{{#if product.condition '===' 'Used'}}http://schema.org/UsedCondition{{/if}}{{#if product.condition '===' 'Refurbished'}}http://schema.org/RefurbishedCondition{{/if}}\">\r\n      <meta itemprop=\"availability\" content=\"{{#unless product.out_of_stock_message}}http://schema.org/InStock{{else}}http://schema.org/OutOfStock{{/unless}}\">\r\n      <div itemprop=\"seller\" itemscope itemtype=\"http://schema.org/Organization\"><meta itemprop=\"name\" content=\"{{settings.store_name}}\"></div>\r\n    </div>\r\n    \r\n    {{> components/products/product/additional-details}}\r\n  </div>\r\n\r\n  <div class=\"single-product-form\">\r\n    <form class=\"form\" method=\"post\" action=\"{{product.cart_url}}\" enctype=\"multipart/form-data\" data-product-options-count=\"{{product.options.length}}\" data-product-form data-validated-form>\r\n      <input type=\"hidden\" name=\"action\" value=\"add\">\r\n      <input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\" data-product-id>\r\n\r\n      \r\n\r\n      <div class=\"single-product-options\">\r\n        {{#if product.options}}\r\n          <div class=\"product-options\" data-product-option-change>\r\n            {{#each product.options}}\r\n              {{{dynamicComponent 'components/products/options'}}}\r\n            {{/each}}\r\n          </div>\r\n        {{/if}}\r\n\r\n        {{#if product.customizations}}\r\n          <div class=\"product-customizations\">\r\n            {{#each product.customizations}}\r\n              {{{dynamicComponent 'components/products/customizations'}}}\r\n            {{/each}}\r\n          </div>\r\n        {{/if}}\r\n      </div>\r\n\r\n      {{#if product.event_date}}\r\n        <div class=\"product-event form-field form-field-date form-required\">\r\n          <span class=\"form-label\">\r\n            <span class=\"form-field-title\">\r\n              {{product.event_date.name}}\r\n              <span class=\"required-text\">{{lang 'common.required'}}</span>\r\n            </span>\r\n            <span class=\"form-field-control\">\r\n              <span class=\"form-select-wrapper\">\r\n                <select class=\"form-input form-select\" name=\"EventDate[Mth]\" required>\r\n                  <option selected disabled>{{lang 'forms.date.month'}}</option>\r\n                  {{#for 1 12}}\r\n                    <option value=\"{{$index}}\">{{getShortMonth $index}}</option>\r\n                  {{/for}}\r\n                </select>\r\n              </span>\r\n              <span class=\"form-select-wrapper\">\r\n                <select class=\"form-input form-select\" name=\"EventDate[Day]\" required>\r\n                  <option selected disabled>{{lang 'forms.date.day'}}</option>\r\n                  {{#for 1 31}}\r\n                    <option value=\"{{$index}}\">{{$index}}</option>\r\n                  {{/for}}\r\n                </select>\r\n              </span>\r\n              <span class=\"form-select-wrapper\">\r\n                <select class=\"form-input form-select\" name=\"EventDate[Yr]\" required>\r\n                  <option selected disabled>{{lang 'forms.date.year'}}</option>\r\n                  {{#if product.event_date.earliest_year '===' product.event_date.latest_year}}\r\n                    <option value=\"{{product.event_date.earliest_year}}\" >{{product.event_date.earliest_year}}</option>\r\n                  {{else}}\r\n                    {{#for product.event_date.earliest_year product.event_date.latest_year}}\r\n                      <option value=\"{{$index}}\" >{{$index}}</option>\r\n                    {{/for}}\r\n                  {{/if}}\r\n                </select>\r\n              </span>\r\n            </span>\r\n          </span>\r\n          {{#if product.event_date.date_start}}\r\n             <span class=\"product-event-bounds\">{{lang 'product.event_bounds' start_date=product.event_date.date_start end_date=product.event_date.date_end}}</span>\r\n          {{/if}}\r\n        </div>\r\n      {{/if}}\r\n\t  \r\n\t  \r\n\t  {{#if product.can_purchase}}\r\n        {{#if product.show_quantity_input}}\r\n          <div class=\"single-product-options product-qty\">            \r\n            <div class=\"product-options\" data-product-option-change=\"\">\r\n              <div class=\"form-field form-field-options form-field-inline form-field-select form-required\" data-product-attribute=\"set-select\">\r\n                <label class=\"form-label\">\r\n                  <span class=\"form-field-title\">Quantity <span class=\"required-text\">*</span>\r\n                  </span>\r\n                  <span class=\"form-field-control\">\r\n                    <div class=\"form-select-wrapper\">\r\n                      <span class=\"form-selected-text\">1</span>\r\n                      <select class=\"form-input form-select\"  name=\"qty[]\" aria-required=\"true\">\r\n                        <option value=\"1\">1</option>\r\n                        <option value=\"2\">2</option>\r\n                        <option value=\"3\">3</option>\r\n                        <option value=\"4\">4</option>\r\n                        <option value=\"5\">5</option>\r\n                        <option value=\"6\">6</option>\r\n                        <option value=\"7\">7</option>\r\n                        <option value=\"8\">8</option>\r\n                        <option value=\"9\">9</option>\r\n                        <option value=\"10\">10</option>\r\n                        <option value=\"11\">11</option>\r\n                        <option value=\"12\">12</option>\r\n                        <option value=\"13\">13</option>\r\n                        <option value=\"14\">14</option>\r\n                        <option value=\"15\">15</option>\r\n                        <option value=\"16\">16</option>\r\n                        <option value=\"17\">17</option>\r\n                        <option value=\"18\">18</option>\r\n                        <option value=\"19\">19</option>\r\n                        <option value=\"20\">20</option>\r\n                        <option value=\"21\">21</option>\r\n                        <option value=\"22\">22</option>\r\n                        <option value=\"23\">23</option>\r\n                        <option value=\"24\">24</option>\r\n                        <option value=\"25\">25</option>\r\n                      </select>\r\n                    </div>\r\n                  </span>\r\n                </label>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        {{else}}\r\n          <input type=\"hidden\" name=\"qty[]\" value=\"1\">\r\n        {{/if}}\r\n      {{/if}}\r\n\t  \r\n    </form>\r\n  </div>\r\n\r\n  <div class=\"form-actions single-product-add-to-cart\" data-product-add>\r\n    {{#if product.can_purchase}}\r\n      <div class=\"product-add-button-wrapper\">\r\n        <button class=\"product-add-button button button-secondary {{#if product.out_of_stock}}disabled{{/if}}\" data-button-purchase {{#if product.pre_order}}data-button-preorder{{/if}} {{#if product.out_of_stock}}disabled{{/if}}>\r\n          <span data-button-text>\r\n            {{#if product.out_of_stock}}\r\n              {{lang 'product.sold_out'}}\r\n            {{else}}\r\n              {{#if product.pre_order}}\r\n                {{lang 'product.pre_order'}}\r\n              {{else}}\r\n                {{lang 'product.add_to_cart'}}\r\n              {{/if}}\r\n            {{/if}}\r\n          </span>\r\n        </button>\r\n        {{> components/common/spinner}}\r\n        {{#if product.can_purchase}}\r\n          {{#if quickshop}}\r\n            <a class=\"view-product-button button button-alt\" href=\"{{product.url}}\">View Product</a>\r\n          {{/if}}\r\n        {{/if}}\r\n      </div>\r\n    {{else}}\r\n      <div class=\"product-add-button-wrapper\">\r\n        <button class=\"product-add-button button disabled\" disabled>\r\n          <span data-button-text>\r\n            {{lang 'product.unavailable'}}\r\n          </span>\r\n        </button>\r\n      </div>\r\n    {{/if}}\r\n  </div>\r\n\r\n    {{#if settings.show_wishlist}}\r\n      <form class=\"single-product-add-to-wishlist\" method=\"post\" action=\"{{product.add_to_wishlist_url}}\">\r\n        <input type=\"hidden\" name=\"variation_id\" value=\"\">\r\n        <button class=\"text-button icon\" type=\"submit\">\r\n          <i class=\"fa fa-heart\"></i> {{lang 'product.add_to_wishlist'}}\r\n        </button>\r\n      </form>\r\n    {{/if}}\r\n\r\n\r\n\r\n{{#if settings.show_product_reviews}}\r\n  {{#each product.reviews.list}}\r\n    <div itemprop=\"review\" itemscope itemtype=\"http://schema.org/Review\">\r\n        <meta itemprop=\"name\" content=\"{{title}}\">\r\n        <meta itemprop=\"reviewBody\" content=\"{{text}}\">\r\n        <div itemprop=\"itemReviewed\" itemscope itemtype=\"http://schema.org/Product\">\r\n            <meta itemprop=\"name\" content=\"{{../product.title}}\"><meta itemprop=\"image\" content=\"{{getImage ../product.main_image}}\">\r\n        </div>\r\n        <div itemprop=\"reviewRating\" itemscope itemtype=\"http://schema.org/Rating\">\r\n            <meta itemprop=\"ratingValue\" content=\"{{rating}}\">\r\n        </div>\r\n        <div itemprop=\"author\" itemscope itemtype=\"http://schema.org/Person\">\r\n            <meta itemprop=\"name\" content=\"{{name}}\">\r\n        </div>\r\n        <div itemprop=\"publisher\" itemscope itemtype=\"http://schema.org/Organization\">\r\n            <meta itemprop=\"name\" content=\"{{../settings.store_name}}\">\r\n        </div>\r\n</div>\r\n  {{/each}}\r\n{{/if}}\r\n\r\n  {{{snippet 'product_details'}}}\r\n  \r\n  \r\n</div>\r\n\r\n{{!-- Product Modals --}}\r\n{{> components/products/bulk-pricing}}\r\n{{> components/products/review-modal}}\r\n","components/products/product/product-slideshow":"<div class=\"product-slideshow {{#if product.images.length '>' 1}}visible-controls{{/if}} {{#if product.images.length '==' 1}}prod-image-1{{/if}}\">\r\n  <div class=\"product-slideshow-images\" data-slideshow-position=\"0\" data-product-images>\r\n    {{#if product.images}}\r\n      {{#each product.images}}\r\n        <img class=\"product-slideshow-image\" data-image-position=\"{{@index}}\" src=\"{{getImage this 'product_zoom'}}\" alt=\"{{this.alt}}\" data-product-images-main>\r\n      {{/each}}\r\n    {{else}}\r\n      <img class=\"product-slideshow-image\" data-image-position=\"{{@index}}\" src=\"{{cdn '/assets/icons/icon-no-image.svg'}}\" alt=\"{{lang 'product.no_image'}}\" class=\"no-image\" data-product-images-main>\r\n    {{/if}}\r\n\r\n    {{#if product.price.saved}}\r\n      <span class=\"product-grid-item-sale-badge\">{{lang 'product.on_sale'}}</span>\r\n    {{/if}}\r\n\r\n    {{> components/common/spinner}}\r\n  </div>\r\n\r\n  {{#if product.images.length \">\" 1}}\r\n    <div class=\"product-thumbnails\">\r\n      {{#each product.images}}\r\n        <span\r\n          class=\"product-thumbnail {{#if @first}}active{{/if}}\"\r\n          style=\"background-image: url({{getImage this 'product_thumbnail'}});\"\r\n          data-image-position=\"{{@index}}\"\r\n          data-high-res=\"{{getImage this 'product_zoom'}}\"\r\n          data-product-thumbnail>\r\n          <img class=\"show-for-sr\" src=\"{{getImage this 'product_thumbnail'}}\" alt=\"{{this.alt}}\">\r\n        </span>\r\n      {{/each}}\r\n    </div>\r\n  {{/if}}\r\n</div>","components/products/quick-shop":"<div class=\"single-product-slideshow-wrapper\">\n  {{> components/products/product/product-slideshow}}\n</div>\n\n<div class=\"product-details-wrapper\">\n  {{> components/products/product/product-details quickshop=true}}\n</div>\n","components/products/ratings":"<div class=\"rating-stars stars-{{size}}\" aria-hidden=\"true\">\r\n  {{#enumerate 1 5}}\r\n    {{#compare ../rating this operator='>='}}\r\n      <span class=\"fa fa-star\"></span>\r\n    {{else}}\r\n      <span class=\"fa fa-star-o\"></span>\r\n    {{/compare}}\r\n  {{/enumerate}}\r\n</div>\r\n{{{snippet 'product_rating'}}}\r\n","components/products/review-modal":"<div id=\"modal-review-form\" class=\"review-modal-container\">\r\n  <a href=\"#\" class=\"modal-close\">\r\n    <i class=\"fa fa-close\"></i>\r\n  </a>\r\n\r\n  <div class=\"modal-body\">\r\n    <div class=\"review-image-column\">\r\n      <figure class=\"review-image-container\">\r\n        {{#if product.main_image}}\r\n          <img src=\"{{getImage product.main_image 'product_item'}}\">\r\n        {{else}}\r\n          <img src=\"{{cdn '/assets/icons/icon-no-image.svg'}}\" alt=\"{{lang 'product.no_image'}}\" class=\"no-image\" data-product-images-main>\r\n        {{/if}}\r\n      </figure>\r\n    </div>\r\n\r\n    <div class=\"review-form-column\">\r\n      <h2 class=\"review-form-heading\">{{lang 'product.reviews.leave_review'}}</h2>\r\n      <form class=\"form form-leave-review\" action=\"{{urls.product.post_review}}\" method=\"post\" data-form-leave-review data-validated-form>\r\n\r\n        {{!-- Clickable Stars --}}\r\n        <div class=\"form-field review-form-stars\">\r\n          <div class=\"show-for-sr\">\r\n            {{lang 'product.reviews.rating_label'}}\r\n          </div>\r\n          <fieldset class=\"input-stars form-fieldset\">\r\n            <input type=\"radio\" id=\"5-stars\" class=\"show-for-sr\" name=\"revrating\" value=\"5\" required data-vd-message-required=\"{{lang 'product.reviews.form.rating_required'}}\">\r\n            <label for=\"5-stars\" class=\"star\" title=\"{{lang 'product.reviews.stars_count' num=5}}\">\r\n              <i class=\"fa fa-star\"></i>\r\n              <span class=\"show-for-sr\">{{lang 'product.reviews.stars_count' num=5}}</span>\r\n            </label>\r\n            <input type=\"radio\" id=\"4-stars\" class=\"show-for-sr\" name=\"revrating\" value=\"4\" required data-vd-message-required=\"{{lang 'product.reviews.form.rating_required'}}\">\r\n            <label for=\"4-stars\" class=\"star\" title=\"{{lang 'product.reviews.stars_count' num=4}}\">\r\n              <i class=\"fa fa-star\"></i>\r\n              <span class=\"show-for-sr\">{{lang 'product.reviews.stars_count' num=4}}</span>\r\n            </label>\r\n            <input type=\"radio\" id=\"3-stars\" class=\"show-for-sr\" name=\"revrating\" value=\"3\" required data-vd-message-required=\"{{lang 'product.reviews.form.rating_required'}}\">\r\n            <label for=\"3-stars\" class=\"star\" title=\"{{lang 'product.reviews.stars_count' num=3}}\">\r\n              <i class=\"fa fa-star\"></i>\r\n              <span class=\"show-for-sr\">{{lang 'product.reviews.stars_count' num=3}}</span>\r\n            </label>\r\n            <input type=\"radio\" id=\"2-stars\" class=\"show-for-sr\" name=\"revrating\" value=\"2\" required data-vd-message-required=\"{{lang 'product.reviews.form.rating_required'}}\">\r\n            <label for=\"2-stars\" class=\"star\" title=\"{{lang 'product.reviews.stars_count' num=2}}\">\r\n              <i class=\"fa fa-star\"></i>\r\n              <span class=\"show-for-sr\">{{lang 'product.reviews.stars_count' num=2}}</span>\r\n            </label>\r\n            <input type=\"radio\" id=\"1-stars\" class=\"show-for-sr\" name=\"revrating\" value=\"1\" required data-vd-message-required=\"{{lang 'product.reviews.form.rating_required'}}\">\r\n            <label for=\"1-stars\" class=\"star\" title=\"{{lang 'product.reviews.stars_count' num=1}}\">\r\n              <i class=\"fa fa-star\"></i>\r\n              <span class=\"show-for-sr\">{{lang 'product.reviews.stars_count' num=1}}</span>\r\n            </label>\r\n          </fieldset>\r\n        </div>\r\n\r\n        {{!-- Name --}}\r\n        {{> core/forms/common/text label=(lang 'product.reviews.form.name') name=\"revfromname\" id=\"revfromname\" required=\"true\" prefill=(lang 'product.reviews.form.name')}}\r\n\r\n        {{!-- Email --}}\r\n        {{> core/forms/common/email label=(lang 'product.reviews.form.email') name=\"email\" id=\"rating-email\" required=\"true\" prefill=(lang 'product.reviews.form.email')}}\r\n\r\n        {{!-- Subject --}}\r\n        {{> core/forms/common/text label=(lang 'product.reviews.form.subject') name=\"revtitle\" id=\"rating-title\" required=\"true\" prefill=(lang 'product.reviews.form.subject')}}\r\n\r\n        {{!-- Comments --}}\r\n        {{> core/forms/common/multiline label=(lang 'product.reviews.form.comments') name=\"revtext\" id=\"rating-comment\" required=\"true\" rows=6 prefill=(lang 'product.reviews.form.comments')}}\r\n\r\n        {{{product.reviews.recaptcha.markup}}}\r\n\r\n        <input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\" />\r\n        <input type=\"hidden\" name=\"action\" value=\"post_review\" />\r\n\r\n        {{#if product.reviews.throttleToken}}\r\n          <input type=\"hidden\" name=\"throttleToken\" value=\"{{product.reviews.throttleToken}}\" />\r\n        {{/if}}\r\n\r\n        <div class=\"form-field form-field-submit\">\r\n          <input type=\"submit\" class=\"form-submit review-modal-submit button button-primary\" value=\"{{lang 'product.reviews.form.submit'}}\">\r\n        </div>\r\n\r\n      </form>\r\n    </div>\r\n\r\n  </div>\r\n</div>\r\n","core/forms/common/email":"\n<div class=\"form-field form-field-common form-field-email {{#if required}} form-required{{/if}}\" id=\"{{id}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{label}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"input-{{id}}\"\n        name=\"{{name}}\"\n        type=\"email\"\n        placeholder=\"{{prefill}}\"\n        {{#if value}}value=\"{{value}}\"{{/if}}\n        {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","core/forms/common/multiline":"{{!\n  Textarea form component\n\n  @param validation_message string\n         Custom validation message\n}}\n<div class=\"form-field form-field-common form-field-multiline {{#if required}}form-required{{/if}}\" id=\"{{id}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{label}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <textarea\n        class=\"form-input\"\n        id=\"textarea-{{id}}\"\n        name=\"{{name}}\"\n        placeholder=\"{{prefill}}\"\n        rows=\"{{rows}}\"\n        {{#if required}}\n          required\n          {{#if validation_message}}\n            data-vd-message-required=\"{{validation_message}}\"\n          {{/if}}\n        {{/if}}\n        aria-required=\"{{required}}\">{{value}}</textarea>\n    </span>\n  </label>\n</div>\n","core/forms/common/text":"\n<div class=\"form-field form-field-common form-field-text {{#if required}} form-required{{/if}}\" id=\"{{id}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{label}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"input-{{id}}\"\n        name=\"{{name}}\"\n        type=\"text\"\n        placeholder=\"{{prefill}}\"\n        {{#if value}}value=\"{{value}}\"{{/if}}\n        {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n"}
