{"components/faceted-search/facets/range":"<form class=\"faceted-search-filter-list show-filter-list {{#if selected }}is-open{{/if}} {{#unless start_collapsed }}is-open{{/unless}}\" method=\"get\" data-faceted-search-range>\r\n  {{#each current_selected_items}}\r\n    <input type=\"hidden\" name=\"{{ param_name }}\" value=\"{{ param_value }}\"/>\r\n  {{/each}}\r\n  \r\n   \r\n    \r\n<div id=\"keypress\"></div>\r\n\t\t\t\r\n  \r\n\r\n  <div class=\"facet-range-input-group\">\r\n      \r\n    <div class=\"facet-range-inputs\">\r\n      <div class=\"form-field faceted-search-range-min\">\r\n        <input\r\n          class=\"form-input\"\r\n          name=\"min_price\"\r\n          id=\"min_price\"\r\n          min=\"0\"\r\n          type=\"number\"\r\n          value=\"0\"\r\n          placeholder=\"{{ lang 'category.sidebar.min_price'}}\"\r\n          required\r\n        />\r\n      </div>\r\n\r\n      <div class=\"form-field faceted-search-range-max\">\r\n        <input\r\n          class=\"form-input\"\r\n          name=\"max_price\"\r\n          id=\"max_price\"\r\n          min=\"0\"\r\n          type=\"number\"\r\n          value=\"5000\"\r\n          placeholder=\"{{ lang 'category.sidebar.max_price'}}\"\r\n          required\r\n        />\r\n      </div>\r\n    </div>\r\n\r\n    <input class=\"button button-secondary button-small facet-submit\" type=\"submit\" value=\"{{lang 'category.sidebar.update'}}\">\r\n\r\n  </div>\r\n</form>\r\n\r\n\r\n\t\t\t\r\n"}
