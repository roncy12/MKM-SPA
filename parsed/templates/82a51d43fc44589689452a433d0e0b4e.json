{"components/common/rating":"\n{{#enumerate 1 5}}\n  <i class=\"icon icon-star rating-star {{#compare ../rating this operator='<'}}empty{{/compare}}\"></i>\n{{/enumerate}}\n","components/products/review":"<div class=\"product-review\">\n\n  <h4 class=\"product-review-title\">{{title}}</h4>\n  <p class=\"product-review-date\">{{date}}</p>\n\n  {{#if settings.show_product_rating}}\n    <div class=\"product-review-rating\">\n      {{> components/common/rating rating=rating}}\n    </div>\n  {{/if}}\n\n  <p class=\"review-comment\">{{text}}</p>\n\n</div>\n"}
