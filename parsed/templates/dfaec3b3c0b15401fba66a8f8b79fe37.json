{"components/cart/cart-preview":"{{#if cart.items}}<span class=\"cart-count\">{{cart.quantity}}</span>{{/if}}\n\n<div class=\"cart-preview-wrap\">\n\n  {{#if cart.items}}\n    <div class=\"cart-preview-body\">\n      <ul class=\"cart-preview-list\">\n        {{#each cart.items}}\n          <li class=\"cart-preview-list-item\">\n\n            <div class=\"cart-preview-list-item-image-wrap\">\n              <div class=\"cart-preview-list-item-image\">\n                {{#if type '===' 'GiftCertificate'}}\n                  <a href=\"javascript:void(0)\">\n                    <img src=\"{{cdn '/assets/icons/giftcard-image.svg'}}\" alt=\"{{recipient}}\">\n                  </a>\n                {{else}}\n                  <a href=\"{{link}}\">\n                    {{#if image}}\n                      <img src=\"{{getImage image 'product_thumbnail'}}\" alt=\"{{name}}\" />\n                    {{else}}\n                      <img src=\"{{cdn '/assets/icons/icon-no-image.svg'}}\" alt=\"{{lang 'product.no_image'}}\">\n                    {{/if}}\n                  </a>\n                {{/if}}\n              </div>\n            </div>\n\n            <div class=\"cart-preview-list-item-details\">\n              <div class=\"cart-preview-list-item-name\">\n                <a href=\"{{link}}\" alt=\"{{name}}\">{{name}}</a>\n              </div>\n\n              <div class=\"cart-preview-list-item-price\">\n                {{{price.formatted}}} <span class=\"cart-preview-list-item-price-quantity\">&times;{{quantity}}</span>\n              </div>\n            </div>\n\n          </li>\n        {{/each}}\n      </ul>\n    </div>\n\n    <div class=\"cart-preview-subtotal\">\n      <div class=\"label\">{{lang 'cart.checkout.subtotal'}}</div>\n      <div class=\"total\">{{cart.sub_total.formatted}}</div>\n    </div>\n\n    <div class=\"cart-preview-footer\">\n      <a href=\"{{urls.cart}}\" class=\"button\">{{lang 'header.view_cart'}}</a>\n      <a href=\"{{urls.checkout.single_address}}\" class=\"button button-alt\">{{lang 'cart.checkout_label'}}</a>\n    </div>\n\n  {{else}}\n    <p class=\"cart-preview-empty\">{{lang \"cart.checkout.empty_cart\"}}</p>\n  {{/if}}\n</div>\n","components/currency-selector":"{{#if currency_selector.currencies.length '>' 1}}\n  <div class=\"currency-selector-wrapper\">\n    <span class=\"show-for-sr\">{{currency_selector.active_currency_name}}</span>\n    <div class=\"form-select-wrapper\">\n      <select class=\"{{#if is-mobile}}mobile-{{/if}}currency-selector\" data-model=\"CurrencySelector\" dir=\"rtl\">\n        {{#each currency_selector.currencies}}\n          <option value=\"{{{switch_url}}}\" {{#if is_active}}selected{{/if}}>\n            {{name}}\n          </option>\n        {{/each}}\n      </select>\n    </div>\n  </div>\n{{/if}}\n","components/navigation":"<section class=\"navigation-wrap\">\n  <nav class=\"navigation\">\n\n    <button class=\"navigation-toggle-mobile-wrapper close\">\n      <span class=\"navigation-toggle-mobile\"><i class=\"fa fa-close\"></i></span>\n    </button>\n\n    <div class=\"mobile-branding\">\n      {{#if settings.store_logo.image}}\n        <a class=\"header-branding-logo\" href=\"{{urls.home}}\">\n          <img class=\"header-branding-image\" src=\"{{getImage settings.store_logo.image 'logo'}}\" alt=\"{{settings.store_logo.title}}\">\n        </a>\n      {{else}}\n        <h1 class=\"header-branding-text\">\n          <a href=\"{{urls.home}}\">{{settings.store_logo.title}}</a>\n        </h1>\n      {{/if}}\n    </div>\n\n    <ul class=\"mobile-actions\">\n\n      <li class=\"mobile-action-item search-toggle\">\n        <a href=\"#\"><i class=\"fa fa-search\"></i></a>\n      </li>\n\n      <li class=\"mobile-action-item cart-count-preview\">\n        <a href=\"{{urls.cart}}\">\n          <i class=\"icon-cart\"></i>\n        </a>\n        <div class=\"cart-preview\">\n          {{> components/cart/cart-preview}}\n        </div>\n      </li>\n\n      {{#if settings.account_creation_enabled}}\n        <li class=\"mobile-action-item user-account\">\n          {{#if customer}}\n            <a class=\"navUser-action\" href=\"{{urls.account.index}}\"><i class=\"icon-user\"></i></a>\n          {{else}}\n            <a class=\"navUser-action\" href=\"{{urls.auth.login}}\"><i class=\"icon-user\"></i></a>\n          {{/if}}\n        </li>\n      {{/if}}\n\n    </ul>\n\n    <div class=\"mobile-search-wrapper search-form-wrapper\">\n      <form class=\"search-form\" action=\"{{urls.search}}\">\n        <input class=\"search-input\" name=\"search_query\" placeholder=\"{{lang 'header.search'}}\">\n      </form>\n    </div>\n\n    <ul class=\"navigation-tier nav-menu\">\n\n      {{#each categories}}\n        <li class=\"nav-menu-item {{#if children}}has-dropdown{{/if}}\">\n          {{> components/navigation-item}}\n        </li>\n      {{/each}}\n\n      {{#if settings.account_creation_enabled}}\n        <li class=\"nav-menu-item mobile-list-item\">\n          {{#if customer}}\n            <a href=\"{{urls.account.index}}\">{{lang 'header.logged_in_as' name=customer.name}}</a>\n            <a href=\"{{urls.auth.logout}}\">{{lang 'header.log_out' }}</a>\n          {{else}}\n            <a href=\"{{urls.auth.login}}\">{{lang 'header.log_in'}}</a>\n          {{/if}}\n        </li>\n      {{/if}}\n\n    </ul>\n\n    <div class=\"header-currency-selector\">\n      {{> components/currency-selector}}\n    </div>\n\n  </nav>\n\n  <div class=\"navigation-overlay\"></div>\n\n</section>\n","components/navigation-item":"\n<a href=\"{{url}}\">{{name}}</a>{{#if children}}<i class=\"nav-icon\"></i>{{/if}}\n{{#if children}}\n  <ul class=\"navigation-tier nav-submenu\">\n    {{#each children}}\n      <li class=\"nav-submenu-item {{#if children}}has-dropdown{{/if}}\">\n        {{> components/navigation-item}}\n      </li>\n    {{/each}}\n  </ul>\n{{/if}}\n"}
