{"components/snippets/social-feeds":"<section class=\"layout-block has-description {{#if theme_settings.homepage_show_newsletter}}no-border{{/if}}\">\n\n  <h2 class=\"section-title\">{{lang 'blog.section_title'}}</h2>\n  <div class=\"section-description\">\n    <p>{{lang 'blog.description'}}</p>\n  </div>\n\n  <div class=\"social-feeds\">\n    <div class=\"social-feeds-wrap\">\n      <div class=\"social-feeds-row\">\n\n        <div class=\"social-feed\">\n          <h3 class=\"social-heading\"><i class=\"icon-rss\"></i> {{lang 'home.latest_blog'}}</h3>\n          <ul class=\"social-blog-posts\">\n            {{#each blog.recent_posts}}\n              <li class=\"recent-blog-post\">\n                <p>\n                  <a href=\"{{url}}\">{{title}}</a>\n                  <span class=\"recent-blog-post-date\">{{date_published}}</span>\n                </p>\n              </li>\n            {{/each}}\n          </ul>\n          <p class=\"social-byline\">{{{lang 'social-widgets.blog_byline' link='/blog'}}}</p>\n        </div>\n\n      </div>\n    </div>\n  </div>\n\n</section>\n"}
