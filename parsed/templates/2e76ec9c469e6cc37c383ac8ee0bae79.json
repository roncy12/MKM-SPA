{"components/common/forms/selectortext":"\n<div class=\"form-field\">\n  <noscript>\n    <input type=\"text\" name=\"{{name}}\" value=\"{{value}}\" class=\"form-input {{class}}\" aria-required=\"{{required}}\" {{#if style}}style=\"{{style}}\" {{/if}} />\n  </noscript>\n\n  <label class=\"form-label\" for=\"{{id}}\">{{label}}</label>\n  <select name=\"{{name}}\" class=\"{{class}}\" aria-required=\"{{required}}\" id=\"{{id}}\" {{#if style}}style=\"{{style}}\" {{/if}}>\n    {{#if options}}\n      <option value=\"\">{{chooseprefix}}</option>\n      {{#each options}}\n        <option {{#equals ../options.value value}} selected {{/equals}}value=\"{{value}}\">{{label}}</option>\n      {{/each}}\n    {{/if}}\n  </select>\n</div>\n"}
