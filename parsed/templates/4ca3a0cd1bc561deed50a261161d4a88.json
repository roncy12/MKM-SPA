{"components/category/filters":"{{#if category.faceted_search_enabled}}\n\n  <a href=\"#\" class=\"button button-alt faceted-sidebar-toggle\">{{lang 'search.faceted.toggle_filters'}}</a>\n\n  {{#if category.selected.items.length}}\n    {{> components/faceted-search/facet-selected category.selected}}\n  {{/if }}\n\n  {{> components/faceted-search/facet-inputs facets=category.facets}}\n{{/if }}\n","components/faceted-search/facet-inputs":"<div class=\"faceted-search\">\n  {{#each facets}}\n    {{#if visible}}\n      {{#if items.length}}\n        <div class=\"faceted-search-filter filter-type-{{type}}\">\n          <h5 class=\"faceted-search-title show-filter\">{{ title }} <span class=\"svg-icon\"><svg class=\"svg-nav-icon\"><use xlink:href=\"#icon-collapse\"></use></svg></span></h5>\n          {{{dynamicComponent 'components/faceted-search/facets'}}}\n        </div>\n      {{/if}}\n\n      {{#compare facet 'price'}}\n        <div class=\"faceted-search-filter filter-type-{{type}}\">\n          <h5 class=\"faceted-search-title show-filter\">{{ title }} <span class=\"svg-icon\"><svg class=\"svg-nav-icon\"><use xlink:href=\"#icon-collapse\"></use></svg></span></h5>\n          {{{dynamicComponent 'components/faceted-search/facets'}}}\n        </div>\n      {{/compare}}\n    {{/if}}\n  {{/each}}\n</div>\n","components/faceted-search/facet-selected":"<div class=\"facet-selected-filters\">\n  <ul class=\"selected-filter-items\">\n    {{#each items}}\n      <li>\n        <a href=\"{{url}}\" title=\"{{title}}\" class=\"selected-filter-item\" rel=\"nofollow\" data-faceted-search-facet>\n          {{title}}\n          <span class=\"remove\">&times;</span>\n        </a>\n      </li>\n    {{/each}}\n  </ul>\n\n  <a href=\"{{remove_all_url}}\" class=\"clear-all\" rel=\"nofollow\" data-faceted-search-facet>{{lang 'search.faceted.selected.clear_all'}}</a>\n</div>\n","components/faceted-search/facets/hierarchy":"<ul class=\"faceted-search-filter-list show-filter-list {{#if selected }}is-open{{/if}} {{#unless start_collapsed }}is-open{{/unless}}\" data-count=\"{{items_count}}\">\n  {{#each items}}\n    <li class=\"faceted-search-item {{#if has_sub_categories }}has-children{{/if}}\">\n      <a href=\"{{url}}\" rel=\"nofollow\" data-faceted-search-facet>\n        <input type=\"checkbox\" class=\"facet-checkbox\" {{#if selected}}checked{{/if}}>\n        <span class=\"faceted-search-item-label\">\n          {{title}}\n          {{#if ../show_product_counts}}<span class=\"faceted-search-item-count\">({{ count }})</span>{{/if}}\n        </span>\n      </a>\n    </li>\n  {{/each}}\n</ul>\n","components/faceted-search/facets/multi":"<ul class=\"faceted-search-filter-list show-filter-list {{#if selected }}is-open{{/if}} {{#unless start_collapsed }}is-open{{/unless}} {{#compare items_count total_items operator='<'}}has-additional-filters{{/compare}}\" data-count=\"{{items_count}}\">\n  {{#each items}}\n    <li class=\"faceted-search-item {{#compare @index items_count operator='>='}}hidden{{/compare}}\">\n      <a href=\"{{url}}\" title=\"{{title}}\" rel=\"nofollow\" data-faceted-search-facet>\n        <input type=\"checkbox\" class=\"facet-checkbox\" {{#if selected}}checked{{/if}}>\n        <span class=\"faceted-search-item-label\">\n          {{title}}\n          {{#if ../show_product_counts}}<span class=\"faceted-search-item-count\">({{ count }})</span>{{/if}}\n        </span>\n      </a>\n    </li>\n  {{/each}}\n</ul>\n","components/faceted-search/facets/range":"<form class=\"faceted-search-filter-list show-filter-list {{#if selected }}is-open{{/if}} {{#unless start_collapsed }}is-open{{/unless}}\" method=\"get\" data-faceted-search-range>\r\n  {{#each current_selected_items}}\r\n    <input type=\"hidden\" name=\"{{ param_name }}\" value=\"{{ param_value }}\"/>\r\n  {{/each}}\r\n  \r\n   \r\n    \r\n<div id=\"keypress\"></div>\r\n\t\t\t\r\n  \r\n\r\n  <div class=\"facet-range-input-group\">\r\n      \r\n    <div class=\"facet-range-inputs\">\r\n      <div class=\"form-field faceted-search-range-min\">\r\n        <input\r\n          class=\"form-input\"\r\n          name=\"min_price\"\r\n          id=\"min_price\"\r\n          min=\"0\"\r\n          type=\"number\"\r\n          value=\"0\"\r\n          placeholder=\"{{ lang 'category.sidebar.min_price'}}\"\r\n          required\r\n        />\r\n      </div>\r\n\r\n      <div class=\"form-field faceted-search-range-max\">\r\n        <input\r\n          class=\"form-input\"\r\n          name=\"max_price\"\r\n          id=\"max_price\"\r\n          min=\"0\"\r\n          type=\"number\"\r\n          value=\"5000\"\r\n          placeholder=\"{{ lang 'category.sidebar.max_price'}}\"\r\n          required\r\n        />\r\n      </div>\r\n    </div>\r\n\r\n    <input class=\"button button-secondary button-small facet-submit\" type=\"submit\" value=\"{{lang 'category.sidebar.update'}}\">\r\n\r\n  </div>\r\n</form>\r\n\r\n\r\n\t\t\t\r\n","components/faceted-search/facets/rating":"<ul class=\"faceted-search-filter-list show-filter-list {{#if selected }}is-open{{/if}} {{#unless start_collapsed }}is-open{{/unless}}\">\n  {{#each items}}\n    <li class=\"faceted-search-item\">\n      <a href=\"{{url}}\" title=\"{{lang 'product.reviews.stars_count' NUM=title}}\" rel=\"nofollow\" data-faceted-search-facet>\n\n        <input type=\"checkbox\" class=\"facet-checkbox\" {{#if selected}}checked{{/if}}>\n\n        <span class=\"faceted-search-item-label facet-rating\">\n\n          {{#if title '==' '1'}}\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star\"></span>\n            <span class=\"fa fa-star\"></span>\n            <span class=\"fa fa-star\"></span>\n            <span class=\"fa fa-star\"></span>\n          {{/if}}\n\n          {{#if title '==' '2'}}\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star\"></span>\n            <span class=\"fa fa-star\"></span>\n            <span class=\"fa fa-star\"></span>\n          {{/if}}\n\n          {{#if title '==' '3'}}\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star\"></span>\n            <span class=\"fa fa-star\"></span>\n          {{/if}}\n\n          {{#if title '==' '4'}}\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star\"></span>\n          {{/if}}\n\n          {{#if title '==' '5'}}\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star checked\"></span>\n            <span class=\"fa fa-star checked\"></span>\n          {{/if}}\n\n          {{#if ../show_product_counts}}<span class=\"faceted-search-item-count\">({{ count }})</span>{{/if}}\n\n        </span>\n\n      </a>\n    </li>\n  {{/each}}\n</ul>\n"}
