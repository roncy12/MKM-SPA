{"components/common/pagination":"<div class=\"pagination-container\">\r\n  <div class=\"pagination\">\r\n\r\n    <output class=\"pagination-count\">\r\n      {{#if links.length '>' 1}}\r\n        <span class=\"pages-lbl\">Pages: </span>\r\n        {{#each links}}\r\n          {{#if number '===' ../current}}\r\n            <span class=\"pagination-number pagination-current\"><a href=\"{{url}}\">{{number}}</a></span>\r\n          {{else}}\r\n            <span class=\"pagination-number\"><a href=\"{{url}}\">{{number}}</a></span>\r\n          {{/if}}\r\n        {{/each}}\r\n      {{else}}\r\n        <span class=\"pagination-number pagination-current\">1</span>\r\n      {{/if}}\r\n    </output>\r\n\r\n    {{#if previous}}\r\n      {{#if is_review}}\r\n      <a class=\"pagination-item previous\" href=\"{{previous}}#product-reviews\" rel=\"nofollow\" data-faceted-search-facet>\r\n      {{else}}\r\n      <a class=\"pagination-item previous\" href=\"{{previous}}\" rel=\"nofollow\" data-faceted-search-facet>\r\n      {{/if}}\r\n        <i class=\"fa fa-angle-left\" aria-hidden=\"true\"></i>\r\n        {{lang 'common.pagination.previous'}}\r\n      </a>\r\n    {{/if}}\r\n    {{#if next}}\r\n      {{#if is_review}}\r\n      <a class=\"pagination-item next\" href=\"{{next}}#product-reviews\" rel=\"nofollow\" data-faceted-search-facet>\r\n      {{else}}\r\n      <a class=\"pagination-item next\" href=\"{{next}}\" rel=\"nofollow\" data-faceted-search-facet>\r\n      {{/if}}\r\n        {{lang 'common.pagination.next'}}\r\n        <i class=\"fa fa-angle-right\" aria-hidden=\"true\"></i>\r\n      </a>\r\n    {{/if}}\r\n  </div>\r\n</div>\r\n","components/common/spinner":"<div class=\"spinner\">\n  <div class=\"spinner-circle1 spinner-circle\"></div>\n  <div class=\"spinner-circle2 spinner-circle\"></div>\n  <div class=\"spinner-circle3 spinner-circle\"></div>\n  <div class=\"spinner-circle4 spinner-circle\"></div>\n  <div class=\"spinner-circle5 spinner-circle\"></div>\n  <div class=\"spinner-circle6 spinner-circle\"></div>\n  <div class=\"spinner-circle7 spinner-circle\"></div>\n  <div class=\"spinner-circle8 spinner-circle\"></div>\n  <div class=\"spinner-circle9 spinner-circle\"></div>\n  <div class=\"spinner-circle10 spinner-circle\"></div>\n  <div class=\"spinner-circle11 spinner-circle\"></div>\n  <div class=\"spinner-circle12 spinner-circle\"></div>\n</div>\n","components/products/grid":"<form action=\"/compare\" class=\"product-grid-list-items {{#if show_compare}}compare-enabled{{/if}} {{#if quickshop}}quickshop-enabled{{/if}} {{rowcount}}\" method=\"POST\" data-product-compare>\r\n  {{#unless products.length}}\r\n    <div class=\"page-container\">\r\n      <div class=\"product-grid-no-results\">\r\n        {{> core/alerts/alert-info (lang 'products.no_products')}}\r\n      </div>\r\n    </div>\r\n  {{/unless}}\r\n\r\n  {{#each products}}\r\n    {{> components/products/product-item show_compare=../show_compare show_rating=../settings.show_product_rating theme_settings=../theme_settings crop=../crop}}\r\n  {{/each}}\r\n\r\n  {{#if show_compare}}\r\n    <div class=\"compare-tab\" id=\"compare-tab\">\r\n      <h5 class=\"item-limit\">{{lang 'compare.compare_up_to_x_items' num=4}}</h5>\r\n      <ul class=\"compare-items-list\" id=\"compare-items-wrapper\"></ul>\r\n      <div class=\"compare-tab-actions\">\r\n        <input type=\"submit\" value=\"{{lang 'compare.compare_submit'}}\" class=\"button compare-submit\">\r\n        <a href=\"#\" class=\"button button-alt clear-compare\">{{lang 'compare.clear_compare' num=4}}</a>\r\n      </div>\r\n    </div>\r\n  {{/if}}\r\n</form>\r\n","components/products/price":"<div class=\"product-price\">\n  {{#if price.with_tax}}\n    <span class=\"price-with-tax\">\n      <span data-product-price-wrapper=\"with-tax\">\n        <span data-product-price>\n          {{price.with_tax.formatted}}\n        </span>\n\n        {{#if price.saved}}\n          <span class=\"price-rrp\" data-product-rrp>\n            {{price.rrp_with_tax.formatted}}\n          </span>\n        {{/if}}\n      </span>\n\n      {{#if price.without_tax}}\n        <span class=\"price-tax-label\">{{lang 'product.including_tax'}}</span>\n      {{/if}}\n    </span>\n  {{/if}}\n\n  {{#if price.without_tax}}\n    <span class=\"price-without-tax\">\n      <span data-product-price-wrapper=\"without-tax\">\n        <span data-product-price>\n          {{price.without_tax.formatted}}\n        </span>\n\n        {{#if price.saved}}\n          <span class=\"price-rrp\" data-product-rrp>\n            {{price.rrp_without_tax.formatted}}\n          </span>\n        {{/if}}\n      </span>\n\n      {{#if price.with_tax}}\n        <span class=\"price-tax-label\">{{lang 'product.excluding_tax'}}</span>\n      {{/if}}\n    </span>\n  {{/if}}\n\n  {{#if show_savings}}\n    {{#if price.saved}}\n      <span class=\"price-saved\" data-product-price-saved>\n        ({{lang 'product.you_save'}} {{price.saved.formatted}})\n      </span>\n    {{/if}}\n  {{/if}}\n</div>\n","components/products/product-item":"<!--<div id=\"ak2-load\"></div>\r\n<script src=\"https://app.akin2.io/widget/widget-generator.js?s=51&h=d41d8cd98f00&limit=12&search={{name}}\" type=\"text/javascript\" async defer>\r\n</script>\r\n<link rel=\"stylesheet\" type=\"text/css\" href=\"https://app.akin2.io/widget/basic.css\">-->\r\n<article class=\"product-grid-item\">\r\n  {{#if out_of_stock_message.length}}\r\n    <span class=\"product-grid-item-sale-badge stock-out\">{{out_of_stock_message}}</span>\r\n  {{else}}\r\n    {{#if price.saved}}\r\n      <span class=\"product-grid-item-sale-badge\">{{lang 'product.on_sale'}}</span>\r\n    {{/if}}\r\n  {{/if}}\r\n  <figure class=\"product-grid-item-thumbnail {{#if crop}}cropped-thumbnail aspect-{{crop}}{{/if}}\">\r\n    <a class=\"{{#if crop}}cropped-thumbnail-block{{/if}}\" href=\"{{url}}\" title=\"{{name}}\"\r\n      style=\"background-image:url({{#if image}}{{getImage image 'product_item'}}{{else}}{{cdn '/assets/icons/icon-no-image.svg'}}{{/if}});\">\r\n\r\n      {{#if image}}\r\n        <img src=\"{{getImage image 'product_item'}}\" alt=\"{{image.alt}}\">\r\n      {{else}}\r\n        <img src=\"{{cdn '/assets/icons/icon-no-image.svg'}}\" alt=\"{{lang 'product.no_image'}}\">\r\n      {{/if}}\r\n    </a>\r\n\r\n    {{#if show_compare}}\r\n      <div class=\"product-item-compare\">\r\n        <div class=\"form-field form-field-inline form-field-checkbox\">\r\n          <div class=\"form-field-control\">\r\n            <label class=\"form-label\" for=\"compare-{{id}}\">\r\n              <input type=\"checkbox\" class=\"form-input form-checkbox\" name=\"products[]\" value=\"{{id}}\" id=\"compare-{{id}}\">\r\n              <span class=\"form-label-text\">{{lang \"products.compare\"}}</span>\r\n            </label>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    {{/if}}\r\n\r\n    <span class=\"quick-shop-trigger\" data-quick-shop-trigger=\"{{id}}\"><i class=\"fa fa-shopping-cart\"></i></span>\r\n  </figure>\r\n\r\n  <div class=\"product-grid-item-details\">\r\n    <h3 class=\"product-item-title\">\r\n      <a href=\"{{url}}\" title=\"{{name}}\">\r\n        {{name}}\r\n      </a>\r\n    </h3>\r\n\r\n    <div class=\"product-grid-item-price\">\r\n      {{> components/products/price price=price}}\r\n    </div>\r\n  </div>\r\n\r\n</article>\r\n","components/search/product-index":"{{> components/products/grid\n  products=product_results.products\n  rowcount=theme_settings.products_per_row\n  show_compare=product_results.show_compare\n  quickshop=theme_settings.show_quick_shop\n  crop=theme_settings.grid_aspect_ratio}}\n\n{{#if pagination.product_results.links.length}}\n  {{> components/common/pagination pagination.product_results}}\n{{/if}}\n\n<div class=\"progress-overlay\">\n  <div class=\"inner\">\n    {{> components/common/spinner}}\n  </div>\n</div>\n","core/alerts/alert-info":"<div class=\"alert alert-info\">\n  {{#if dismissable}}<a class=\"alert-dismiss\">&times;</a>{{/if}}\n\n  <div class=\"alert-message\">\n    {{#if message}}\n      {{{message}}}\n    {{else}}\n      {{{this}}}\n    {{/if}}\n  </div>\n</div>\n"}
