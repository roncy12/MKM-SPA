{"components/cart/cart-preview":"{{#if cart.items}}<span class=\"cart-count\">{{cart.quantity}}</span>{{/if}}\n\n<div class=\"cart-preview-wrap\">\n\n  {{#if cart.items}}\n    <div class=\"cart-preview-body\">\n      <ul class=\"cart-preview-list\">\n        {{#each cart.items}}\n          <li class=\"cart-preview-list-item\">\n\n            <div class=\"cart-preview-list-item-image-wrap\">\n              <div class=\"cart-preview-list-item-image\">\n                {{#if type '===' 'GiftCertificate'}}\n                  <a href=\"javascript:void(0)\">\n                    <img src=\"{{cdn '/assets/icons/giftcard-image.svg'}}\" alt=\"{{recipient}}\">\n                  </a>\n                {{else}}\n                  <a href=\"{{link}}\">\n                    {{#if image}}\n                      <img src=\"{{getImage image 'product_thumbnail'}}\" alt=\"{{name}}\" />\n                    {{else}}\n                      <img src=\"{{cdn '/assets/icons/icon-no-image.svg'}}\" alt=\"{{lang 'product.no_image'}}\">\n                    {{/if}}\n                  </a>\n                {{/if}}\n              </div>\n            </div>\n\n            <div class=\"cart-preview-list-item-details\">\n              <div class=\"cart-preview-list-item-name\">\n                <a href=\"{{link}}\" alt=\"{{name}}\">{{name}}</a>\n              </div>\n\n              <div class=\"cart-preview-list-item-price\">\n                {{{price.formatted}}} <span class=\"cart-preview-list-item-price-quantity\">&times;{{quantity}}</span>\n              </div>\n            </div>\n\n          </li>\n        {{/each}}\n      </ul>\n    </div>\n\n    <div class=\"cart-preview-subtotal\">\n      <div class=\"label\">{{lang 'cart.checkout.subtotal'}}</div>\n      <div class=\"total\">{{cart.sub_total.formatted}}</div>\n    </div>\n\n    <div class=\"cart-preview-footer\">\n      <a href=\"{{urls.cart}}\" class=\"button\">{{lang 'header.view_cart'}}</a>\n      <a href=\"{{urls.checkout.single_address}}\" class=\"button button-alt\">{{lang 'cart.checkout_label'}}</a>\n    </div>\n\n  {{else}}\n    <p class=\"cart-preview-empty\">{{lang \"cart.checkout.empty_cart\"}}</p>\n  {{/if}}\n</div>\n","components/header/header-tools":"<div class=\"header-tools\">\r\n  {{> components/header/top}}\r\n  <ul class=\"header-tools-inner\">\r\n\r\n    <li class=\"header-tools-item navigation-toggle-mobile-container\">\r\n      <button class=\"navigation-toggle-mobile-wrapper\">\r\n        <span class=\"navigation-toggle-mobile\"><i class=\"fa fa-bars\"></i></span>\r\n      </button>\r\n    </li>\r\n\r\n    {{#if settings.account_creation_enabled}}\r\n      <li class=\"header-tools-item\">\r\n        {{#if customer}}\r\n          <a href=\"{{urls.account.index}}\">\r\n            <i class=\"fa fa-user\"></i>\r\n            <span class=\"label\">{{lang 'header.account'}}</span>\r\n          </a>\r\n        {{else}}\r\n          <a href=\"{{urls.auth.login}}\">\r\n            <i class=\"fa fa-user\"></i>\r\n            <span class=\"label\">{{lang 'header.account'}}</span>\r\n          </a>\r\n        {{/if}}\r\n      </li>\r\n    {{/if}}\r\n\r\n    <li class=\"header-tools-item\">\r\n      <a href=\"{{urls.account.orders.all}}\">\r\n        <i class=\"fa fa-list-ul\"></i> \r\n        <span class=\"label\">{{lang 'header.order_status'}}</span>\r\n      </a>\r\n    </li>\r\n\r\n    {{#if settings.show_wishlist}}\r\n    <li class=\"header-tools-item\">\r\n      <a href=\"{{urls.account.wishlists.all}}\">\r\n        <i class=\"fa fa-heart\"></i>\r\n        <span class=\"label\">{{lang 'header.wishlist'}}</span>\r\n      </a>\r\n    </li>\r\n    {{/if}}\r\n\r\n    {{#if settings.gift_certificates_enabled}}\r\n    <li class=\"header-tools-item\">\r\n        <a href=\"{{urls.gift_certificate.purchase}}\">\r\n          <i class=\"fa fa-gift\"></i>\r\n          <span class=\"label\">{{lang 'header.gift_cert'}}</span>\r\n        </a>\r\n    </li>\r\n    {{/if}}\r\n\r\n    <li class=\"header-tools-item cart-toggle {{#if cart.items}}has-cart-items{{/if}}\">\r\n      <a class=\"cart-preview-trigger\" href=\"#\">\r\n        <i class=\"fa fa-shopping-cart\"></i>\r\n        <span class=\"label\">{{lang 'header.cart'}}</span>\r\n      </a>\r\n      <div class=\"cart-preview\">\r\n        {{> components/cart/cart-preview}}\r\n      </div>\r\n    </li>  \r\n\r\n  </ul>\r\n</div>\r\n","components/header/top":"  <ul class=\"header-tools-left\">\n    <li><span class=\"lbl\">{{lang 'global.phone_number.label'}}: </span><a href=\"tel:>{{lang 'global.phone_number.tel'}}\">{{lang 'global.phone_number.val'}}</a></li>\n    <li><a href=\"/customer-service/\">Customer Service</a></li>\n  </ul>\n"}
