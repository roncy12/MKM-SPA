{"components/faceted-search/facets/multi":"<ul class=\"faceted-search-filter-list show-filter-list {{#if selected }}is-open{{/if}} {{#unless start_collapsed }}is-open{{/unless}} {{#compare items_count total_items operator='<'}}has-additional-filters{{/compare}}\" data-count=\"{{items_count}}\">\n  {{#each items}}\n    <li class=\"faceted-search-item {{#compare @index items_count operator='>='}}hidden{{/compare}}\">\n      <a href=\"{{url}}\" title=\"{{title}}\" rel=\"nofollow\" data-faceted-search-facet>\n        <input type=\"checkbox\" class=\"facet-checkbox\" {{#if selected}}checked{{/if}}>\n        <span class=\"faceted-search-item-label\">\n          {{title}}\n          {{#if ../show_product_counts}}<span class=\"faceted-search-item-count\">({{ count }})</span>{{/if}}\n        </span>\n      </a>\n    </li>\n  {{/each}}\n</ul>\n"}
